    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/config-angular": "^9.1.1",
    "@rollup/plugin-babel": "^5.0.3",
